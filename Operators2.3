package com.gmail.com;

import java.util.Scanner;

public class Mail {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Ведите номер билета");
		int n = sc.nextInt();
		int a = n / 1000;
		int b = n % 1000 / 100;
		int c = n % 100 / 10;
		int d = n % 10 % 10;
		int x = a + b;
		int y = c + d;
		int x1 = x / 10;
		int x2 = x % 10;
		int z = x1 + x2;
		int y1 = y / 10;
		int y2 = y % 10;
		int s = y1 + y2;
		if ((x > 10) && (y > 10)) {
			System.out.println("Билет счастливый");
		} else if (z == s) {
			System.out.println("Билет счастливый");
		} else
			System.out.println("Билет не счастливый");
	}

}
